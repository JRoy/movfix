{
  "manifest_version": 3,
  "name": "movfix",
  "version": "1.0.0",
  "description": "Fixes QuickTime/MOV videos not playing in Chrome",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
